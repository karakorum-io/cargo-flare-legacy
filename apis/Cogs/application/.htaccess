<IfModule authz_core_module>
    Require all denied
Header set Access-Control-Allow-Origin "*"
Header add Access-Control-Allow-Credentials "true"
</IfModule>
<IfModule !authz_core_module>
Header set Access-Control-Allow-Origin "*"
Header add Access-Control-Allow-Credentials "true"
    Deny from all
</IfModule>