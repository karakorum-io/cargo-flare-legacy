/* top menu */
.headline {
	clear: both;
	margin: 0 0 1em;
	color: #fff;
}

.menu {
	display: block;
	float: left;
	width: 100%;
	background: url('../images/bg_menu.gif') no-repeat right top;
}
.menu li {
	display: block;
	float: left;
	height:46px;
}
.menu .first {
	width:9px;
	font-size: 15px;
	line-height:26px;
	background:#fff url(../images/bg_menu_f.gif) no-repeat;
}
.menu .last {
	width:9px;
	font-size: 15px;
	line-height:26px;
	background:#fff url(../images/bg_menu_l.gif) no-repeat;
}
.menu li a {
	display: block;
