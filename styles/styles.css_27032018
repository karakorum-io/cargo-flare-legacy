@charset "utf-8";
/* basic tags */

green{
    color:green !important;
}

red{
    color:red !important;
}

* {
	margin: 0;
	padding: 0;
}
ul, ol {
	list-style: none;
}

.required {
    color: red;
}
body {
    margin: 0;
	padding: 0 0 2em;
    font-family: "FuturaLight", Aria, Helvetica, sans-serif;
    font-size: 12px;
    color: #767676;
	background: #fff url(../images/bg.gif) repeat-x;
}
td {
    font-family: "FuturaLight", Arial, Helvetica, sans-serif;
    font-size: 12px;
    color: #767676;
}
input, select, textarea {
    font-family: "FuturaLight", Arial, Helvetica, sans-serif;
    font-size: 12px;
    color: #767676;
}
hr {
	margin: 24px 0;
	font-size: 0;
	height: 0;
	background: none;
	border: none;
	border-top: 2px solid #ddd;
}
em {
	color: #767676;
	font-style: normal
}
p {
	line-height: 1.6em;
	padding-bottom: .7em;
}

/* basic tags with classes */


/* links classes */
a img {
    border:0;
}

a:link, a:visited, a:active, .like-link {
    color: #008ec2;
    outline: none;
	text-decoration: none;
	cursor: pointer;
}
a:hover, .like-link:hover {
    color: #8fa75e;
}

.like-link {
    color: #008ec2;
    outline: none;
	text-decoration: none;
	cursor: pointer;
	border-bottom: 1px dashed  #008ec2;
}
.like-link:hover {
    color: #008ec2;
    border-bottom:none;
}


a.green:link, a.green:visited, a.green:active {
    color: #839711;
    outline: none;
	text-decoration: underline;
}
a.green:hover {
    color: #008EC2;
}





